{
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://widget.intercom.io; style-src 'self' 'unsafe-inline'; connect-src 'self' /t https://eu.i.posthog.com https://api-iam.intercom.io wss://nexus-websocket-a.intercom.io; img-src 'self' data: https:; font-src 'self' https://js.intercomcdn.com; frame-src https://www.intercom.com"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/t/static/:path*",
      "destination": "https://eu-assets.i.posthog.com/static/:path*"
    },
    {
      "source": "/t/:path*",
      "destination": "https://eu.i.posthog.com/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/instagram",
      "destination": "/?utm_source=instagram&utm_medium=bio",
      "permanent": false
    },
    {
      "source": "/facebook",
      "destination": "/?utm_source=facebook&utm_medium=page",
      "permanent": false
    },
    {
      "source": "/producthunt",
      "destination": "/?utm_source=producthunt&utm_medium=launch",
      "permanent": false
    },
    {
      "source": "/twitter",
      "destination": "/?utm_source=twitter&utm_medium=profile",
      "permanent": false
    },
    {
      "source": "/press",
      "destination": "/?utm_source=press&utm_medium=email",
      "permanent": false
    }
  ]
}
